{
  "name": "cheshirecode-next-boilerplate",
  "version": "1.0.0",
  "description": "Nextjs with emotion",
  "bin": "server/index.js",
  "keywords": [
    "nextjs",
    "esm",
    "emotion",
    "module-resolver"
  ],
  "author": "dac4158@gmail.com",
  "license": "UNLICENSE",
  "private": true,
  "scripts": {
    "dev:lite": "next dev",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "cross-env NODE_ENV=production node -r esm server",
    "deploy": "rm -rf client/.next client/bundles && npx vercel alias $(npx vercel . --token $NOW_TOKEN) cheshirecode-next-boilerplate",
    "analyze": "cross-env NODE_ENV=production ANALYZE=both yarn build",
    "count": "npx cloc --vcs=git --exclude-ext=json --by-file-by-lang --skip-uniqueness",
    "prepare": "husky install",
    "info": "next info"
  },
  "browser": {
    "express": false,
    "esm": false,
    "webpack": false
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11",
    "emotion": "^11",
    "next": "latest",
    "prop-types": "^15.6.2",
    "ramda": "^0.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reset-css": "^4.0.1",
    "svg-sprite-loader": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.16.5",
    "@babel/preset-react": "^7.16.7",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/eslint-plugin": "^11.7.0",
    "@next/bundle-analyzer": "^12",
    "@types/dotenv": "^8.2.0",
    "babel-plugin-import": "^1.13.1",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8",
    "eslint-config-recommended": "^4.0.0",
    "eslint-import-resolver-babel-module": "^5.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.0.0",
    "prettier": "^2",
    "snyk": "^1.279.1",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "{./,server}/**/*.{js,jsx,json,css}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "snyk": true
}
